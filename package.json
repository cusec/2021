    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
