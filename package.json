    "@types/react": "^16.9.47",
    "@types/react-hamburger-menu": "0.0.3",
    "@types/react-linkify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
